@{
    Layout = "~/Views/Apps/Layout/_Layout_Search_Charts_Table2.cshtml";
}

 <p class="fl">
	        <label for="start_time">开始日期：</label>
	        <input type="text" id="start_time" name="start_time" class="big_input add_clear_btn " />
        </p>
         <p class="fl ml20">
	        <label for="end_time">结束日期：</label>
	        <input type="text" id="end_time" name="end_time" class="big_input add_clear_btn " />
        </p>
        <a class="data_search_btn fl pos_btn big_btn ml20">搜索</a>
<div class="attention_help_wrap">
    <a class="app_help">Info</a>
    @* 帮助信息 *@
    @Html.Raw(ViewBag.Desc)
    <a class="download"><img src="/Static/image/common/icon/download.png" />表格数据下载</a>
</div>
@section BottomResource{
    @if (CT.BI.WEB.BASE.BaseController.IsResoucePack())
     { 
        @Scripts.Render("~/bundles/loginusers/charts_table/js")
     }
     else
     {
        <script src="/Static/js/apps/partial/charts/login_users/charts_byday.js"></script>
        <script src="/Static/js/apps/partial/charts/login_users/charts_perday.js"></script>
        <script src="/Static/js/apps/partial/table/login_users/table_byday.js"></script>
        <script src="/Static/js/apps/partial/table/login_users/table_compare.js"></script>
        <script src="/Static/js/apps/partial/table/login_users/table_perday.js"></script>
     }
    @Scripts.Render("~/bundles/highstock")
    <script src="/Static/js/apps/operate/login_users/op_tcnations_byday.js"></script>
    <script src="/Static/js/layout/nations.js"></script>
     <script>
         $(function () {
             var state = @ViewBag.AttentionState;
            nations.init(state);
        });
    </script>
}